{
  "name": "libxmljs2",
  "author": "marudor",
  "contributors": [
    "Jeff Smick",
    "Marco Rogers"
  ],
  "binary": {
    "module_name": "xmljs",
    "module_path": "./build/Release/",
    "host": "https://github.com",
    "remote_path": "./marudor/libxmljs2/releases/download/v{version}/",
    "package_name": "{node_abi}-{platform}-{arch}-{libc}.tar.gz"
  },
  "description": "libxml bindings for v8 javascript engine",
  "version": "0.28.0",
  "scripts": {
    "build": "node-pre-gyp install --build-from-source",
    "install": "node-pre-gyp install --fallback-to-build --loglevel http",
    "test": "node --expose_gc ./node_modules/jest/bin/jest.js",
    "lint": "eslint index.js test",
    "tsd": "tsd",
    "package": "node-pre-gyp package 2>&1"
  },
  "repository": {
    "type": "git",
    "url": "http://github.com/marudor/libxmljs2.git"
  },
  "bugs": {
    "url": "http://github.com/marudor/libxmljs2/issues"
  },
  "main": "./index",
  "license": "MIT",
  "engines": {
    "node": ">=12"
  },
  "files": [
    "index.js",
    "index.d.ts",
    "src",
    "lib",
    "vendor",
    "binding.gyp",
    "Makefile"
  ],
  "dependencies": {
    "@mapbox/node-pre-gyp": "^1.0.5",
    "bindings": "~1.5.0",
    "nan": "~2.15.0"
  },
  "devDependencies": {
    "babel-eslint": "^10.1.0",
    "eslint": "^7.32.0",
    "eslint-config-marudor": "^8.0.1",
    "jest": "^27.1.0",
    "jest-watch-typeahead": "^0.6.4",
    "prettier": "^2.3.2",
    "tsd": "^0.17.0",
    "typescript": "^4.4.2"
  }
}
